{
  "name": "@axa-fr/oidc-service-worker",
  "version": "6.17.0",
  "private": true,
  "type": "module",
  "main": "dist/OidcServiceWorker.js",
  "types": "dist/OidcServiceWorker.d.ts",
  "description": "OpenID Connect & OAuth authentication service worker",
  "files": [
    "dist",
    "src",
    "package.json",
    "package-lock.json"
  ],
  "keywords": [
    "oidc",
    "OpenID Connect",
    "openid",
    "oauth2",
    "oauth"
  ],
  "scripts": {
    "copy": "cpy --flat ./src/OidcTrustedDomains.js ./dist/",
    "build": "tsc && vite build && npm run copy",
    "test": "vitest run",
    "test:watch": "vitest --coverage",
    "coverage": "vitest run --coverage",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "lint": "eslint src"
  },
  "dependencies": {
    "base64-js": "1.5.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitest/coverage-c8": "^0.29.2",
    "copyfiles": "2.4.1",
    "eslint": "^8.26.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "msw": "0.46.1",
    "typescript": "4.8.2",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2",
    "vitest": "^0.29.2"
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

{
  "name": "@finchy/vanilla-oidc",
  "version": "6.22.0",
  "private": false,
  "main": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "OpenID Connect & OAuth authentication using native javascript only, compatible with angular, react, vue, svelte, next, etc.",
  "files": [
    "dist",
    "bin",
    "src",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AxaGuilDEv/react-oidc.git"
  },
  "dependencies": {
    "base64-js": "1.5.1"
  },
  "devDependencies": {
    "@axa-fr/oidc-service-worker": "6.17.0",
    "@types/node": "17.0.15",
    "copyfiles": "2.4.1",
    "cpy-cli": "^4.2.0",
    "rimraf": "3.0.2",
    "typescript": "4.5.5"
  },
  "keywords": [
    "oidc",
    "OpenID Connect",
    "openid",
    "oauth2",
    "oauth",
    "vanilla"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf src",
    "copy:dist": "cpy ../oidc-service-worker/dist/** ./dist/oidc-service-worker",
    "copy": "cpy --flat ../react/src/oidc/vanilla/*.ts ./src",
    "prepare": "npm run clean && npm run copy && tsc --build && npm run copy:dist",
    "postinstall": "node ./bin/copy.js"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gitHead": "209e4aadfb2f50a39f3f8b68afb5e30cfd1a7753"
}
